{
  "name": "sample-node",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "./node_modules/.bin/nodemon -w src --exec \"babel-node src/www.js --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dest --source-maps",
    "start": "npm run build && node dest/www.js",
    "just_start": "node dest/www.js",
    "lint": "eslint src",
    "lint-test": "node_modules/.bin/eslint test",
    "test": "./node_modules/.bin/mocha test --compilers js:babel-register --exit --require babel-polyfill --reporter spec --require test/include/chai.js",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --require -- -u exports --timeout 10000 --exit --compilers js:babel-register --recursive test",
    "go": "npm run clean && npm run lint && npm run lint-test && npm run test && npm run build"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "^2.5.7",
    "eslint": "^4.16.0",
    "exphbs": "^1.1.0",
    "express": "~4.15.5",
    "express-handlebars": "^3.0.0",
    "form-data": "^2.3.1",
    "html": "^1.0.0",
    "insecure": "0.0.4",
    "mongodb": "^3.0.1",
    "mongoose": "^4.13.9",
    "morgan": "~1.9.0",
    "pretty-data": "^0.40.0",
    "pug": "^2.0.0-rc.4",
    "pug-lint": "^2.5.0",
    "request": "^2.83.0",
    "request-debug": "^0.2.0",
    "serve-favicon": "~2.4.5",
    "simple-xml2json": "^1.2.3",
    "xmldom": "^0.1.27",
    "xmljson": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.0.0"
  }
}
