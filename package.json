{
  "name": "sample-node",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "./node_modules/.bin/nodemon -w src --exec \"babel-node src/www.js --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dest --source-maps",
    "start": "npm run build && node dest/www.js",
    "just_start": "node dest/www.js",
    "lint": "eslint src",
    "lint-test": "node_modules/.bin/eslint test",
    "test": "./node_modules/.bin/mocha test --compilers js:babel-register --exit --require babel-polyfill --reporter spec --require test/include/chai.js",
    "cover": "npm run test && ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --require -- -u exports --timeout 10000 --exit --compilers js:babel-register --recursive test",
    "go": "npm run lint && npm run lint-test && npm run build"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "~1.18.2",
    "chai-datetime": "^1.5.0",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "^2.5.7",
    "enum": "^2.5.0",
    "eslint": "^4.16.0",
    "exphbs": "^1.1.0",
    "express": "~4.15.5",
    "express-handlebars": "^3.0.0",
    "express-interceptor": "^1.2.0",
    "express-session": "^1.15.6",
    "form-data": "^2.3.1",
    "handlebars": "^4.0.11",
    "handlebars-intl": "^1.1.2",
    "hashmap": "^2.3.0",
    "html": "^1.0.0",
    "insecure": "0.0.4",
    "mongodb": "^3.0.1",
    "mongoose": "^4.13.9",
    "morgan": "~1.9.0",
    "multiparty": "^4.1.3",
    "pretty-data": "^0.40.0",
    "pug": "^2.0.0-rc.4",
    "pug-lint": "^2.5.0",
    "request": "^2.83.0",
    "request-debug": "^0.2.0",
    "serve-favicon": "~2.4.5",
    "simple-xml2json": "^1.2.3",
    "stream-to-array": "^2.3.0",
    "stream-to-string": "^1.1.0",
    "xml2js": "^0.4.19",
    "xmldom": "^0.1.27",
    "xmljson": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.0.0"
  }
}
